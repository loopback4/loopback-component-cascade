{
  "name": "loopback-component-cascade",
  "version": "0.1.0",
  "description": "Loopback component models cascade create, update, delete",
  "keywords": [
    "loopback-extension",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "lb-clean dist *.tsbuildinfo",
    "prebuild": "npm run clean",
    "build": "lb-tsc --outDir dist",
    "build:watch": "lb-tsc --watch --outDir dist",
    "start": "node .",
    "start:watch": "nodemon .",
    "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/loopback4/loopback-component-cascade"
  },
  "author": "KoLiBer",
  "license": "MIT",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist"
  ],
  "dependencies": {
    "@loopback/boot": "^2.4.1",
    "@loopback/context": "^3.9.4",
    "@loopback/core": "^2.9.3",
    "@loopback/repository": "^2.11.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.0",
    "@loopback/testlab": "^3.2.2",
    "@types/node": "^10.17.28",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.1",
    "typescript": "~3.9.7"
  }
}
